<div class="modal-header">
    <a class="close" href="#">x</a>
    <h2>{{ label }}</h2>
</div>

<div class="modal-body">
    {{#if video}}
        <video controls="controls" height="100%" width="100%">
            <source src="{{ src }}">
        </video>
    {{else}}
        <iframe src="{{ src }}" height="100%" width="100%"></iframe>
    {{/if}}
</div>