
<div data-action="play">

	<span class="song-name"> {{ label }}</span>

	<div data-action="more" class="pull-right song-item-options">
		<i class="icon-chevron-down icon-white"></i>
	</div>

	<div class="pull-right swap-button swap-down" data-action="swap-down">
		<i class="icon-arrow-down icon-white"></i>
	</div>

	<div class="pull-right swap-button swap-up" data-action="swap-up">
		<i class="icon-arrow-up icon-white"></i>
	</div>

</div>