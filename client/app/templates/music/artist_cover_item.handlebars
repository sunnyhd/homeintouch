<div class="artistContainer" data-touchable="black">
    {{#if thumbnail}}
        <img src="{{ thumbnail }}" onerror="this.src='img/svg/media/defaultArtist.svg'">
    {{else}}
    	<img src="img/svg/media/defaultArtist.svg">
    {{/if}}

    <div class="artist-caption">
    	<span>{{ label }}</span>
    </div>
</div>